<div>
    {{#if profucts.length = 0}}
        {{#each products}}
        <div>
            <p>title: {{this.title}}</p>
            <p>description: {{this.description}}</p>
            <p>price: {{this.price}}</p>
            <p>thumbnail: {{this.thumbnail}}</p>
            <p>code: {{this.code}}</p>
            <p>stock: {{this.stock}}</p>
            </br>
        </div>
        {{/each}}
    {{else}}
        <div> 
            <p>No hay Productos en la Lista</p>
        </div>
    {{/if}}
</div>